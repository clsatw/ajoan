  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:zh-hant": "ng s --aot --i18n-file src/locale/messages.zh-hant.xlf --i18n-format xlf --i18n-locale zh-hant",
    "build": "ng build --prod --base-href \"/en-US/\" --output-path dist/en-US",
    "build:zh-hant": "ng build --base-href \"/zh-hant/\" --prod --i18n-file=src/locale/messages.zh-hant.xlf --output-path dist/zh-hant --i18n-format xlf --i18n-locale zh-hant",
    "build:zh-hans": "ng build --base-href \"/zh-hans/\" --prod --i18n-file=src/locale/messages.zh-hans.xlf --output-path dist/zh-hans --i18n-format xlf --i18n-locale zh-hans",
    "build:fr": "ng build --aot --output-path dist/fr --base-href /customer-app/fr/ --i18nFile=src/locale/messages.fr.xlf --i18nFormat=xlf --locale=fr",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract": "ng xi18n --outputPath=locale",
    "start-server": "./node_modules/.bin/ts-node ./server.ts --secure",
    "server": "./node_modules/.bin/nodemon -w ./ --ext \".ts\" --exec \"npm run start-server\""
  },
  "private": true,
